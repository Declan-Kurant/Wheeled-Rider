<%= form_for([@vehicle, @category]) do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :author %>
  <%= f.text_field :author %>

  <%= f.label :num_replies %>
  <%= f.text_field :num_replies %>

  <%= f.label :last_reply %>
  <%= f.text_field :last_reply %>

  <%= f.label :vehicle %>
  <%= f.text_field :vehicle %>

  <%= f.submit %>

<% end %>
